{
  "name": "webuild.sg",
  "version": "2.10.30",
  "homepage": "http://webuild.sg",
  "keywords": "developer, designer, maker, open source, open events, open community",
  "private": true,
  "scripts": {
    "start": "node app",
    "test": "grunt travis --verbose && mocha test --recursive && mocha test --recursive --require blanket --reporter html-cov > coverage.html",
    "postinstall": "bower install && grunt build",
    "routine": "grunt cleanup && npm outdated && bower list && npm-check-updates -u && npm i && grunt && npm test && npm outdated"
  },
  "dependencies": {
    "body-parser": "1.13.1",
    "bower": "1.4.1",
    "buffer-crc32": "0.2.5",
    "cli-color": "1.0.0",
    "compression": "1.5.0",
    "cookie-signature": "1.0.6",
    "cors": "^2.7.1",
    "errorhandler": "1.4.0",
    "express": "4.12.4",
    "get-urls": "4.0.1",
    "github": "0.2.4",
    "grunt-cli": "0.1.13",
    "grunt-contrib-stylus": "0.21.0",
    "grunt-contrib-uglify": "0.9.1",
    "grunt-jsbeautifier": "0.2.10",
    "htmlstrip-native": "0.1.1",
    "ical": "0.4.0",
    "ical-generator": "notthetup/ical-generator",
    "is-number": "2.0.2",
    "jade": "1.11.0",
    "jsonfile": "2.0.1",
    "markdown": "0.5.0",
    "mess": "0.1.2",
    "moment-timezone": "0.4.0",
    "newrelic": "1.20.1",
    "passport": "0.2.2",
    "passport-auth0": "0.2.1",
    "prequest": "0.0.3",
    "promise": "7.0.3",
    "request": "2.58.0",
    "serve-favicon": "2.3.0",
    "sitemap": "^0.9.0",
    "word-overlap": "2.3.2"
  },
  "devDependencies": {
    "blanket": "1.1.7",
    "bower": "1.4.1",
    "chai": "3.0.0",
    "coveralls": "2.11.2",
    "grunt": "0.4.5",
    "grunt-bump": "0.3.1",
    "grunt-cli": "0.1.13",
    "grunt-contrib-clean": "0.6.0",
    "grunt-contrib-csslint": "0.4.0",
    "grunt-contrib-jshint": "0.11.2",
    "grunt-contrib-stylus": "0.21.0",
    "grunt-contrib-uglify": "0.9.1",
    "grunt-jsbeautifier": "0.2.10",
    "grunt-jscs": "1.8.0",
    "mocha": "2.2.5",
    "mocha-lcov-reporter": "0.0.2",
    "nconf": "0.7.1"
  },
  "engines": {
    "iojs": "2.2.x"
  },
  "config": {
    "blanket": {
      "pattern": [
        "archives/index.js",
        "countdown/index.js",
        "events/eventbriteEvents.js",
        "events/facebookEvents.js",
        "events/index.js",
        "events/meetupEvents.js",
        "events/utils.js",
        "repos/index.js"
      ],
      "data-cover-never": "node_modules"
    }
  }
}
